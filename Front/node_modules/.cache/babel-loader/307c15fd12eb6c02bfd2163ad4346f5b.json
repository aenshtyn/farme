ADA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,OAAJ;;AAAA,sBAAPU,QAAO;AAAA;AAAA;AAAA,OAAb;;AAIA,QAAME,QAAO,GAAI,SAAXA,QAAW,GAAM;AACrBP,gBAAU,CAACQ,KAAX,GAAmB,CAACR,UAAU,CAACQ,KAA/B;AACD,KAFD;;AAIAlB,aAAS,CAACY,OAAD,CAAT;AAEA,WAAO;AACLP,UAAI,EAAJA,IADK;AAELQ,YAAM,EAANA,MAFK;AAGLE,cAAQ,EAARA,QAHK;AAILE,cAAQ,EAARA,QAJK;AAKLP,gBAAU,EAAVA,UALK;AAMLE,aAAO,EAAPA,OANK;AAOLD,UAAI,EAAJA;AAPK,KAAP;AASD;AAlCY,CAAf","names":["useMilks","useCows","onMounted","ref","reactive","name","setup","form","milking_time","day","volume","cow_id","cowsToggle","cows","getCows","errors","storeMilk","saveMilk","milk","showCows","value"],"sourceRoot":"","sources":["/Users/aenshtyn/Desktop/Code/Personal/Projects/farm/Front/src/components/Production.vue"],"sourcesContent":["<template>\n  <div class=\"mt-2 mb-6 text-sm text-red-600\" v-if=\"errors !== ''\">\n    {{ errors }}\n  </div>\n\n  <form @submit.prevent=\"saveMilk\">\n    <div class=\"submit-form\">\n      <!-- <div v-if=\"!submitted\"> -->\n      <div class=\"form-group\">\n        <label for=\"breed\">Milking Time</label>\n        <div class=\"select\">\n          <select v-model=\"form.milking_time\">\n            <option value=\"AM\">AM</option>\n            <option value=\"PM\">PM</option>\n          </select>\n          <span class=\"focus\"></span>\n        </div>\n      </div>\n      <div class=\"form-group\">\n        <label for=\"day\">Day</label>\n        <input\n          type=\"date\"\n          class=\"form-control\"\n          id=\"day\"\n          required\n          v-model=\"form.day\"\n          name=\"day\"\n        />\n      </div>\n      <div class=\"form-group\">\n        <label for=\"owner\">Volume</label>\n        <input\n          type=\"text\"\n          class=\"form-control\"\n          id=\"volume\"\n          required\n          v-model=\"form.volume\"\n          name=\"volume\"\n        />\n      </div>\n      <div class=\"form-group\">\n        <label for=\"owner\">Cow</label>\n        <div class=\"select\">\n          <select v-model=\"form.cow_id\">\n            <option v-for=\"cow in cows\" :key=\"cow.id\" :value=\"cow.id\">\n              {{ cow.name }}\n            </option>\n          </select>\n        </div>\n        <span class=\"focus\"></span>\n      </div>\n      <button type=\"submit\" class=\"btn btn-success\">Submit</button>\n    </div>\n  </form>\n  <!-- </div> -->\n</template>\n\n<script>\nimport useMilks from \"../services/MilkDataService\";\nimport useCows from \"../services/AnimalDataService\";\nimport { onMounted } from \"vue\";\nimport { ref, reactive } from \"vue\";\nexport default {\n  name: \"record_milk\",\n  setup() {\n    const form = reactive({\n      milking_time: \"\",\n      day: null,\n      volume: \"\",\n      cow_id: \"\",\n    });\n\n    const cowsToggle = ref(false);\n    const { cows, getCows } = useCows();\n    const { errors, storeMilk } = useMilks();\n    //  const cows = ref([]);\n\n    const saveMilk = async () => {\n      await storeMilk({ milk: { ...form } });\n    };\n\n    const showCows = () => {\n      cowsToggle.value = !cowsToggle.value;\n    };\n\n    onMounted(getCows);\n\n    return {\n      form,\n      errors,\n      saveMilk,\n      showCows,\n      cowsToggle,\n      getCows,\n      cows,\n    };\n  },\n};\n</script>\n<style>\n.submit-form {\n  max-width: 300px;\n  margin: auto;\n}\n</style>\n"]}]}